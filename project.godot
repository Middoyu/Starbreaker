config_version=5

[application]

config/name="☆ Starbreaker ☆"
config/description="Starbreaker is a anti-SHMUP game, designed to break the rules of classics SHMUP gameplay loops in order to create a fun and new experience."
config/version="0.61.5 ALPHA"
config/tags=PackedStringArray("starbreaker")
run/main_scene="res://Game Folder/game_assets/Menus/IntroCine/intro_cine.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="uid://bq5wnk4wiiwor"

[audio]

buses/default_bus_layout="res://z_Export/default_bus_layout.tres"

[autoload]

global="*res://Game Folder/game_components/global_components/Scripts/global.gd"
events="*res://Game Folder/game_components/global_components/Scripts/events.gd"
utility="*res://Game Folder/game_components/global_components/Scripts/utils.gd"
options="*res://Game Folder/game_components/global_components/Scripts/options.gd"
score_event="*res://Game Folder/game_components/global_components/ScoreManager/ScoreEventBus.gd"
cache="*res://Game Folder/game_components/global_components/Scripts/cache.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=4
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/fps=240

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Game Folder/game_assets/Enemies/": "red",
"res://Game Folder/game_assets/Menus/": "yellow",
"res://Game Folder/game_assets/Menus/IntroCine/": "pink",
"res://Game Folder/game_assets/Menus/SFX/": "blue",
"res://Game Folder/game_assets/Menus/Themes/": "purple",
"res://Game Folder/game_assets/Player/": "blue",
"res://Game Folder/game_assets/Shaders/": "purple",
"res://Game Folder/game_assets/Stages/": "green",
"res://z_Export/": "gray"
}

[filesystem]

import/blender/enabled=false

[global_group]

player=""
enemy=""

[importer_defaults]

