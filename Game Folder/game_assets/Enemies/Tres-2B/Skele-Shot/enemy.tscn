[sub_resource type="GDScript" id="GDScript_vvde2"]
script/source = "extends Enemy
@onready var hurtbox: Databox = $Hurtbox
@onready var hitbox: Databox = $Hitbox
@onready var sprite: AnimatedSprite2D = $Sprite
@onready var state_machine = $StateMachine

func _physics_process(_delta: float) -> void:
	move_and_slide()

#region Interaction Handlers
func on_parent_hit(colliding_hitbox : Databox, damage_taken) -> void:
	events.enemy_damaged.emit(self)
	utility.display_damage(damage_taken, self)
	state_machine.current_state.Transistioned.emit(state_machine.current_state, state_machine.hit_state.name)

func on_parent_death(colliding_hitbox : Databox, damage_taken) -> void:
	events.enemy_killed.emit(self)
	utility.display_damage(damage_taken, self)
	state_machine.current_state.Transistioned.emit(state_machine.current_state, state_machine.death_state.name)
	sprite.rotation -= global_position.angle_to_point(global.player.global_position)

func pteam_hurtbox_entered(area):
	area.take_damage(hitbox)
#endregion
"

