@onready var hurtbox: HurtboxComponent = $\"../../HurtboxComponent\"


func enter():
	$\"../../PatternGenerator\".firerate_timer.stop()
	sprite.play_backwards(\"Spawn\")
	hurtbox.queue_free()
	await sprite.animation_finished
	parent.queue_free()
"

