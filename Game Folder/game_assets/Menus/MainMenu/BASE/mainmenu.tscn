script/source = "extends CustomButton


func _process(delta: float) -> void:
	match button_pressed:
		true:
			text = \"Screenshake : ON\"
			options.screenshake_enabled = true
		false:
			text = \"Screenshake : OFF\"
			options.screenshake_enabled = false
