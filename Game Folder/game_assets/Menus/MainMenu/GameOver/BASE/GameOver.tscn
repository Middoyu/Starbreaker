script/source = "extends AudioStreamPlayer

func fade():
	var volume_tween = create_tween().bind_node(self).set_ease(Tween.EASE_OUT).set_trans(Tween.TRANS_CUBIC)
	volume_tween.tween_property(self, \"volume_db\", -60.0, 1.0)
