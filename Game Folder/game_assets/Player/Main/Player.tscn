script/source = "extends Node  # Inherit from Node class

@onready var invincibility_timer := Timer.new()  # Initialize the invincibility timer
@onready var invincibility_override : bool = false  # Flag to control invincibility override
@export var hit_invincibility_duration : float = 1.75  # Duration of invincibility after taking damage

# Called when the node is ready
func _ready() -> void:
	events.connect(\"player_death\", func(damage_taken, colliding_hitbox):
		invincibility_override = true  # Enable invincibility override on player death
	, 2)
	events.connect(\"player_damaged\", func(current_health, damage_taken, colliding_hitbox):
		invincibility_timer.start(hit_invincibility_duration)  # Start the invincibility timer when damaged
	, 3)
	setup_invincibility_timer()  # Initialize the invincibility timer

# Called every physics frame
func _physics_process(delta: float) -> void:
	invincibility_check()  # Check the status of invincibility

# Setup the invincibility timer with default properties
func setup_invincibility_timer():
	invincibility_timer.one_shot = true  # The timer runs only once
	invincibility_timer.autostart = false  # The timer doesn't start automatically
	invincibility_timer.process_callback = Timer.TIMER_PROCESS_PHYSICS  # Use physics process to update
	add_child.call_deferred(invincibility_timer, true)  # Add the timer as a child with deferred call

# Check if the player is currently invincible
func invincibility_check():
	if not invincibility_override:  # If invincibility override is not active
		match invincibility_timer.time_left:  # Check the timer's remaining time
			0.0:
				owner.hurtbox.is_disabled = false  # Disable the hurtbox (player vulnerable)
			_:
				owner.hurtbox.is_disabled = true  # Enable the hurtbox (player invincible)
	else:
		owner.hurtbox.is_disabled = true  # Always enable hurtbox if invincibility override is active
"

