[sub_resource type="GDScript" id="GDScript_haiii"]
resource_name = "Breaker"
script/source = "extends WeaponBase
@onready var lazer_databox: Databox = $BreakerLazer/LazerDatabox
@onready var display: ColorRect = $BreakerLazer/Display
@onready var tickrate: Timer = $BreakerLazer/Tickrate
@onready var breaker_lazer: Node2D = $BreakerLazer
@onready var is_active = false
@export var charge_threshold : int = 75
@export var charge_drain_amount = -5.0

func _ready() -> void:
	display.hide()

func _physics_process(delta: float) -> void:
	breaker_lazer.global_position = player.global_position

func action():
	if global.player.charge >= charge_threshold:
		is_active = true
		tickrate_timeout()
		display.show()

func tickrate_timeout() -> void:
	player.charge = charge_drain_amount
	match lazer_databox.is_disabled:
		false:
			lazer_databox.is_disabled = true
			tickrate.start(0.08)
		true:
			lazer_databox.is_disabled = false
			tickrate.start(0.08)
	if is_zero_approx(global.player.charge):
		tickrate.stop()
		is_active = false
		display.hide()
		lazer_databox.is_disabled = true


func eteam_hurtbox_entered(area: Databox) -> void:
	area.take_damage(lazer_databox)
"

