[sub_resource type="GDScript" id="GDScript_itgeb"]
script/source = "extends Node  # Inherit from the base Node class
@onready var player: Player = $\"..\"

@export var allows_input := false  # Flag to control input
@export var is_movement_disabled := false  # Flag to control movement
@onready var movement_speed : float  # Initial movement speed, should be set up in the editor
@export var max_movement_speed : float  # Maximum movement speed
@export var acceleration : float  # Acceleration rate
@export var friction : float  # Friction rate

# Called when the node is ready
func _ready() -> void:
	# Connect signals to handle player death and tutorial completion
	events.connect(\"player_death\", func(damage_taken, colliding_hitbox):
		allows_input = false  # Disable input on player death
		owner.velocity = Vector2.ZERO  # Stop player movement
	, 2)
	events.connect(\"stage_started\", func():
		is_movement_disabled = false  # Re-enable movement after tutorial completion
	)

# Called every physics frame
func _physics_process(delta: float) -> void:
	look_at_mouse()  # Make the player look at the mouse position
	movement_handler(delta)  # Handle player movement

# Rotate player to face the mouse cursor
func look_at_mouse():
	if allows_input:
		owner.look_at(player.crosshair.global_position)  # Face the mouse if input is allowed

# Handle player movement
func movement_handler(delta):
	if allows_input and not is_movement_disabled:
		var direction = Input.get_vector(\"ui_left\", \"ui_right\", \"ui_up\", \"ui_down\").normalized()
		
		# Apply acceleration if there is a direction
		if direction:
			movement_speed += acceleration * delta
		
		# Apply friction if there is no input direction
		if not direction:
			movement_speed -= friction * delta
		
		# Clamp the movement speed to be within the allowed range
		movement_speed = clamp(movement_speed, 0.0, max_movement_speed)
		
		# Apply the velocity to move the player
		owner.velocity = direction * movement_speed * delta
		
	owner.move_and_slide()  # Ensure physics-based movement
