[sub_resource type="GDScript" id="GDScript_ld5h1"]
resource_name = "secondaries"
script/source = "extends WeaponBase
const MISSILE_PROJ = preload(\"res://Game Folder/game_assets/Player/Secondary/Missile/missile_proj.tscn\")

@export var missile_amount = 7

# Overrides WeaponBase's function to allowing for upgrades.
func spawn_projectile(projectile_path):
	var proj_int = projectile_path.instantiate()
	proj_int.global_position = player.global_position
	global.current_stage.add_child(proj_int)
	proj_int.parent = get_parent()
	upgrade_projectile(proj_int)

func shoot():
	for i in missile_amount:
		spawn_projectile(MISSILE_PROJ)
		global.player.charge = -50 / missile_amount
"

[sub_resource type="GDScript" id="GDScript_0bcgs"]
resource_name = "player_util"
script/source = "extends Node
@onready var parent = get_parent() as Player
@export var charge_threshold : int = 25
@onready var utility_hitbox: Databox = $Utility_Hitbox


func _ready() -> void:
	setup_teleport_hitboxes()

#region Teleportation Functions
func setup_teleport_hitboxes():
	# Disables the hitbox on boot.
	utility_hitbox.is_disabled = true
	
	# Pause to allow children setting up.
	await get_tree().create_timer(0.3).timeout
	
	# Reparents the hitbox to the parent.
	utility_hitbox.reparent(global.player)
	
	# Sets the hitbox global position to the player's position to avoid offsets.
	utility_hitbox.global_position = utility.get_player().global_position

func teleport_action():
	# Detects if the player matches the charge threshold.
	if utility.get_player().charge >= charge_threshold: # Detects if the player matches the charge threshold.
		# Quickly disables the players hurtboxes.
		parent.invincibility_override = true
		parent.player_hurtbox.is_disabled = true
		
		# Teleports the crosshair position.
		get_parent().global_position = utility.get_crosshair().global_position
		utility_hitbox.global_position = utility.get_player().global_position
		
		# Removes the required charge from the player's current charge.
		utility.get_player().charge = -charge_threshold
		
		# Enables the hitbox for the teleport.
		utility_hitbox.is_disabled = false
		
		# Creates a short pause for the collisions to not break.
		await get_tree().create_timer(0.1).timeout
		
		# Disables the hitbox for the teleport.
		utility_hitbox.is_disabled = true
		
		# Enabled the player collisions.
		parent.invincibility_override = false
		parent.player_hurtbox.is_disabled = false

func enemy_team_hitbox_entered(area: Databox) -> void:
	# Damages the enemy.
	area.take_damage(utility_hitbox)

#endregion
"

