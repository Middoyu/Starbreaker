script/source = "extends Node

var is_all_weapons_actionable = true  # Exported boolean to control whether actions are actionable
@export_enum(\"Spray\", \"Shotgun\", \"Longshot\", \"Slash\") var primary_firingmode_selection = \"None\"
@export_enum(\"Spray\", \"Shotgun\", \"Longshot\", \"Slash\") var secondary_firingmode_selection = \"None\"
@export_enum(\"Zip-Dash\", \"DiagBomb\", \"Shine\") var utility_selection = \"None\"
@export_enum(\"BH-7\", \"Overclock\", \"Starman\") var breaker_selection = \"None\"

@export var max_energy := 100.0
@onready var current_energy := max_energy
@export var energy_gain_persecond := 15 / 1.0
@export var energy_perdamage := 5
@export var energy_perkill := 10


@onready var active_primary_firingmode
@onready var active_secondary_firingmode
@onready var active_utility_selection

# Firing Modes
const SPRAY_FIRINGMODE = preload(\"res://Game Folder/game_assets/Player/Weapons/firing_modes/Spray/Base/spray_firingmode.tscn\")
const SHOTGUN_FIRINGMODE = preload(\"res://Game Folder/game_assets/Player/Weapons/firing_modes/Shotgun/Shotgun.tscn\")
const SLASH_FIRINGMODE = preload(\"res://Game Folder/game_assets/Player/Weapons/firing_modes/Slash/Slash.tscn\")

# Utilities
const ZIP_DASH = preload(\"res://Game Folder/game_assets/Player/Weapons/utility/Zip-Dash/zipdash.tscn\")
const SHINE = preload(\"res://Game Folder/game_assets/Player/Weapons/utility/Shine/Shine.tscn\")

func _ready() -> void:
	firingmode_selector(primary_firingmode_selection)
	firingmode_selector(secondary_firingmode_selection)
	utilitymode_selector()
	
	events.connect(\"enemy_damaged\", update_consecutive_hits_counter, 1)
	events.connect(\"enemy_killed\", update_consecutive_hits_counter, 1)
	events.connect(\"enemy_damaged\", func(enemy): call(\"update_energy\", energy_perdamage), 1)
	events.connect(\"enemy_killed\", func(enemy): call(\"update_energy\", energy_perkill), 1)
	events.connect(\"player_death\", func(damage, hb): is_all_weapons_actionable = false, 2)

func firingmode_selector(firingmode : String):
	match(firingmode):
		\"Spray\":
			match(firingmode):
				primary_firingmode_selection:
					active_primary_firingmode = SPRAY_FIRINGMODE.instantiate()
					add_child(active_primary_firingmode)
				secondary_firingmode_selection:
					active_secondary_firingmode = SPRAY_FIRINGMODE.instantiate()
					add_child(active_secondary_firingmode)
		\"Shotgun\":
			match(firingmode):
				primary_firingmode_selection:
					active_primary_firingmode = SHOTGUN_FIRINGMODE.instantiate()
					add_child(active_primary_firingmode)
				secondary_firingmode_selection:
					active_secondary_firingmode = SHOTGUN_FIRINGMODE.instantiate()
					add_child(active_secondary_firingmode)
		\"Longshot\":
			pass
		\"Slash\":
			match(firingmode):
				primary_firingmode_selection:
					active_primary_firingmode = SLASH_FIRINGMODE.instantiate()
					add_child(active_primary_firingmode)
				secondary_firingmode_selection:
					active_secondary_firingmode = SLASH_FIRINGMODE.instantiate()
					add_child(active_secondary_firingmode)
		\"None\":
			pass

func utilitymode_selector():
	match utility_selection:
		\"Zip-Dash\":
			active_utility_selection = ZIP_DASH.instantiate()
			add_child(active_utility_selection)
		\"DiagBomb\":
			pass
		\"Shine\":
			active_utility_selection = SHINE.instantiate()
			add_child(active_utility_selection)


func _physics_process(delta: float) -> void:
	input_code(delta)
	update_energy(15 * delta)


## Charge Mechanics
func update_energy(new_value : float):
	current_energy += new_value
	current_energy = clamp(current_energy, 0, max_energy)
	events.emit_signal(\"energy_updated\", current_energy)

func input_code(delta):
	if is_all_weapons_actionable:
		if active_primary_firingmode != null:
			if Input.is_action_pressed(\"primary\") and active_primary_firingmode.is_weapon_actionable:
				active_primary_firingmode.is_weapon_actionable = false
				active_primary_firingmode.action()
		if active_secondary_firingmode != null:
			if Input.is_action_pressed(\"secondary\") and active_secondary_firingmode.is_weapon_actionable:
				active_secondary_firingmode.is_weapon_actionable = false
				active_secondary_firingmode.action()
		if active_utility_selection != null:
			if Input.is_action_just_pressed(\"shift\") and active_utility_selection.is_weapon_actionable:
				active_utility_selection.action()

@onready var consecutive_hits_target = Node2D
@onready var consecutive_hits = 0

func update_consecutive_hits_counter(target : Node2D):
	if target == consecutive_hits_target:
		consecutive_hits += 1
	if target != consecutive_hits_target:
		consecutive_hits = 1
	consecutive_hits_target = target
	print(str(consecutive_hits_target, consecutive_hits))
"

